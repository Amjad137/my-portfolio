{
  "name": "blogora",
  "version": "0.0.1-5",
  "private": true,
  "scripts": {
    "dev": "yarn env:copy:dev && next dev",
    "dev:offline": "yarn --offline env:copy:dev && yarn --offline next dev",
    "dev:qa": "yarn env:copy:qa && next dev",
    "dev:production": "yarn env:copy:production && next dev",
    "build": "yarn env:copy:dev && next build",
    "build:qa": "yarn env:copy:qa && next build",
    "build:production": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "env:copy:dev": "cp .env.development .env.local",
    "env:copy:qa": "cp .env.qa .env.local",
    "env:copy:production": "cp .env.production .env.local",
    "release:pre": "yarn version --prerelease && yarn extract",
    "release:patch": "yarn version --patch && yarn extract",
    "release:minor": "yarn version --minor && yarn extract",
    "release:major": "yarn version --major && yarn extract",
    "extract": "node -p \"'export const VERSION = ' + JSON.stringify(require('./package.json').version) + ';'\" > src/version.ts && yarn format:fix && git add . && git commit -m 'build: version update' && git push --tags",
    "prepare": "husky"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.832.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.7",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-separator": "^1.1.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@radix-ui/react-toggle": "^1.1.3",
    "@radix-ui/react-toggle-group": "^1.1.3",
    "@radix-ui/react-tooltip": "^1.2.0",
    "@tanstack/react-query": "^5.74.4",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-webcam": "^1.1.0",
    "axios": "^1.9.0",
    "axios-auth-refresh": "^3.3.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "http-status-codes": "^2.3.0",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.525.0",
    "next": "16.0.7",
    "next-themes": "^0.4.6",
    "nextjs-toploader": "^3.8.16",
    "react": "19.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.53.2",
    "react-phone-number-input": "^3.4.12",
    "react-webcam": "^7.2.0",
    "recharts": "^2.15.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "yup": "^1.5.0",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^8",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-sonarjs": "^3.0.2",
    "husky": "^9.1.7",
    "postcss": "^8",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "npm": "Unsupported. Use Yarn instead.",
    "yarn": ">= 1.22.00",
    "node": ">= 18.17.0"
  },
  "packageManager": "yarn@1.22.22",
  "resolutions": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
